{
  "name": "immutabix",
  "version": "1.0.0",
  "author": "Antonis Karamitros @antouank",
  "bugs": {
    "url": "https://github.com/AntouanK/immutabix/issues"
  },
  "dependencies": {
    "consologger": "^1",
    "immutable": "^3",
    "websocket": "^1"
  },
  "devDependencies": {
    "babel": "^4",
    "mocha": "^2",
    "should": "^5"
  },
  "homepage": "https://github.com/AntouanK/immutabix",
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AntouanK/immutabix.git"
  },
  "scripts": {
    "build": "npm run make-space && npm run build-js",
    "build-js": "./node_modules/babel/bin/babel/index.js --loose all --blacklist regenerator,es6.templateLiterals,es6.forOf,spec.blockScopedFunctions,es6.constants --source-maps ./src --out-dir ./dist",
    "make-space": "rm -rf dist && mkdir dist",
    "test": "mocha test/suite.js && mocha test/suite.2.js"
  }
}
